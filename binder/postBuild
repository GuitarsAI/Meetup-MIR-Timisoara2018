jupyter contrib nbextension install --user
jupyter nbextension enable --py widgetsnbextension
jupyter nbextension enable python-markdown/main
jupyter nbextension enable hide_input/main

# Notebooks w/ extensions that auto-run code must be "trusted" to work the first time
jupyter trust meetupTimiVer2.ipynb

add-apt-repository ppa:mscore-ubuntu/mscore-stable
apt-get install musescore

wget "https://ftp.osuosl.org/pub/musescore/releases/MuseScore-2.2.1/MuseScore-2.2.1-x86_64.AppImage"
chmod u+x MuseScore-2.2.1-x86_64.AppImage
./MuseScore-2.2.1-x86_64.AppImage --install
